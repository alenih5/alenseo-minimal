{
    "name": "alenseo/seo-plugin",
    "description": "Alenseo SEO Plugin for WordPress",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Alenseo\\": "includes/"
        },
        "classmap": [
            "includes/"
        ],
        "files": []
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        },
        "no-plugins": true,
        "no-scripts": true,
        "process-timeout": 0,
        "github-protocols": ["https"],
        "github-oauth": {},
        "http-basic": {},
        "store-auths": false,
        "use-github-api": false,
        "disable-tls": false,
        "secure-http": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "archive": {
        "exclude": [
            "tests",
            "docs",
            "examples",
            "*.md",
            "*.txt",
            "*.yml",
            "*.xml",
            "*.json",
            "*.lock",
            ".git*",
            ".editorconfig",
            ".gitattributes",
            ".gitignore",
            ".php_cs",
            ".travis.yml",
            "phpunit.xml",
            "composer.json",
            "composer.lock"
        ]
    }
}
